Func ReadQCNLG($modelqcnlg, $imeiqcnlg) ;check

	Global $spcqcnlg = SendSPC()
	Global $flag1 = 0
	If $spcqcnlg <> False Then
	 	SetProcLanDefinitiveFunc("readqcn" & $indicator)
		Global $finale = 80
		If Not FileExists(@ScriptDir & "\t50.exe") Then
