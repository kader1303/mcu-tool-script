						_GUICtrlEdit_LineScroll($Edit1, 0, _GUICtrlEdit_GetLineCount($Edit1))
						ProcessClose("t50.exe")
						FileDelete(@ScriptDir & "\t50.exe")
						Return False
					EndIf
				Else
					_GUICtrlEdit_SetText($Edit1, GUICtrlRead($Edit1) & $iArrayP[10] & @CRLF)
					_GUICtrlEdit_LineScroll($Edit1, 0, _GUICtrlEdit_GetLineCount($Edit1))
